        &lt;/Profile&gt;</value>
      </setting>
    </Ptv.XServer.Demo.Properties.Settings>
  </userSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
  </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="XLocateWSBinding">
          <security mode="Transport"/>
        </binding>
        <binding name="XLocateWSBinding1"/>
        <binding name="XRouteWSBinding">
          <security mode="Transport"/>
        </binding>
        <binding name="XRouteWSBinding1"/>
        <binding name="XTourWSBinding">
          <security mode="Transport"/>
        </binding>
        <binding name="XTourWSBinding1"/>
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="https://api-eu-test.cloud.ptvgroup.com/xlocate/ws/XLocate" binding="basicHttpBinding" bindingConfiguration="XLocateWSBinding" contract="XlocateService.XLocateWS" name="XLocateWSPort"/>
      <endpoint address="https://api-eu-test.cloud.ptvgroup.com/xroute/ws/XRoute" binding="basicHttpBinding" bindingConfiguration="XRouteWSBinding" contract="XrouteService.XRouteWS" name="XRouteWSPort"/>
      <endpoint address="https://api-eu-test.cloud.ptvgroup.com/xtour/ws/XTour" binding="basicHttpBinding" bindingConfiguration="XTourWSBinding" contract="XtourService.XTourWS" name="XTourWSPort"/>
    </client>
  </system.serviceModel>
 
  <system.diagnostics>
    <sources>
      <source name="WmsMapService" switchName="General" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="Console"/>
          <add name="File"/>
          <remove name="Default"/>
        </listeners>
      </source>

      <source name="XMapTiledProvider" switchName="General" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="Console"/>
          <add name="File"/>
          <remove name="Default"/>
        </listeners>
      </source>

      <source name="RemoteTiledProvider" switchName="General" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="Console"/>
          <add name="File"/>
          <remove name="Default"/>
        </listeners>
      </source>

      <source name="XMapMetaInfo" switchName="General" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="Console"/>
          <add name="File"/>
          <remove name="Default"/>
        </listeners>
      </source>
      
      <!-- For demonstrational reasons, the trace source "ThreadPool" contains an own switch, which can be configured 
      differently compared to the 'General' switch. To provide an individual switch for a dedicated class makes it 
      possible to increase the detail level only for this class, getting a more detailed view of the class' behavior. 
      In case of the switch named 'ThreadPool', the sub node with the corresponding name in the 'switches-node' has to
      be modified to value="Verbose", to get detailed information of the ThreadPool class. -->
      <source name="ThreadPool" switchName="ThreadPool" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="Console"/>
          <add name="File"/>
          <remove name="Default"/>
        </listeners>
      </source>
    </sources>

    <switches>
      <add name="General" value="Error"/>
      <add name="ThreadPool" value="Error"/>
    </switches>
    <sharedListeners>
      <add name="Console" type="System.Diagnostics.ConsoleTraceListener">
        <filter type="System.Diagnostics.EventTypeFilter" initializeData="All"/>
      </add>
      <add name="File" type="System.Diagnostics.TextWriterTraceListener" initializeData="myListener.log">
        <filter type="System.Diagnostics.EventTypeFilter" initializeData="All"/>
      </add>
    </sharedListeners>
  </system.diagnostics>
</configuration>
