<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="Ptv.XServer.Demo.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
    </sectionGroup>
  </configSections>
  <userSettings>
    <Ptv.XServer.Demo.Properties.Settings>
      <setting name="XUrl" serializeAs="String">
        <value>api-eu-test</value>
      </setting>
      <setting name="XToken" serializeAs="String">
        <value/>
      </setting>
      <setting name="BingKey" serializeAs="String">
        <value/>
      </setting>
      <setting name="HereAppId" serializeAs="String">
        <value/>
      </setting>
      <setting name="HereAppCode" serializeAs="String">
        <value/>
      </setting>
      <setting name="IntroSkipped" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="DragDropIntroSkipped" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="FeatureLayerEurope" serializeAs="String">
        <value>        &lt;Profile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"&gt;
          &lt;FeatureLayer majorVersion="1" minorVersion="0"&gt;
            &lt;GlobalSettings enableTimeDependency="true"/&gt;
            &lt;Themes&gt;
              &lt;Theme id="PTV_TrafficIncidents" enabled="false" priorityLevel="0"/&gt;
              &lt;Theme id="PTV_TruckAttributes" enabled="false" priorityLevel="0"/&gt;
              &lt;Theme id="PTV_RestrictionZones" enabled="false" priorityLevel="0"/&gt;
              &lt;Theme id="PTV_SpeedPatterns" enabled="false" priorityLevel="0"/&gt;
            &lt;/Themes&gt;
          &lt;/FeatureLayer&gt;
          &lt;Routing majorVersion="2" minorVersion="0"&gt;
            &lt;Course&gt;
              &lt;AdditionalDataRules enabled="true" /&gt;
              &lt;DynamicRouting limitDynamicSpeedToStaticSpeed="false" /&gt;
            &lt;/Course&gt;
            &lt;Vehicle&gt;
              &lt;Load hazardousGoodsType="HAZARDOUS" /&gt;
            &lt;/Vehicle&gt;
          &lt;/Routing&gt;
        &lt;/Profile&gt;
</value>
      </setting>
      <setting name="FeatureLayerNorthAmerica" serializeAs="String">
        <value>&lt;Profile
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"&gt;
    &lt;FeatureLayer majorVersion="1" minorVersion="0"&gt;
        &lt;GlobalSettings enableTimeDependency="true"/&gt;
        &lt;Themes&gt;
            &lt;Theme id="PTV_PreferredRoutes" enabled="false" priorityLevel="0"&gt;
                &lt;PropertyValue id='preferredRouteType' value='1'/&gt;
                &lt;PropertyValue id='preferredRouteType' value='2'/&gt;
                &lt;PropertyValue id='preferredRouteType' value='3'/&gt;
                &lt;PropertyValue id='preferredRouteType' value='4'/&gt;
                &lt;PropertyValue id='preferredRouteType' value='5'/&gt;
                &lt;PropertyValue id='preferredRouteType' value='7'/&gt;
                &lt;PropertyValue id='preferredRouteType' value='18'/&gt;
                &lt;PropertyValue id='preferredRouteType' value='21'/&gt;
                &lt;PropertyValue id='preferredRouteType' value='22'/&gt;
            &lt;/Theme&gt;
            &lt;Theme id="PTV_SpeedPatterns" enabled="false" priorityLevel="0"/&gt;
        &lt;/Themes&gt;
    &lt;/FeatureLayer&gt;
    &lt;Routing majorVersion="2" minorVersion="0"&gt;
        &lt;Course&gt;
            &lt;AdditionalDataRules enabled="true" /&gt;
            &lt;DynamicRouting limitDynamicSpeedToStaticSpeed="false" /&gt;
        &lt;/Course&gt;
        &lt;Vehicle&gt;
            &lt;Load hazardousGoodsType="HAZARDOUS" /&gt;
        &lt;/Vehicle&gt;
    &lt;/Routing&gt;
&lt;/Profile&gt;</value>
      </setting>
      <setting name="FeatureLayerAustralia" serializeAs="String">
        <value>&lt;Profile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"&gt;
          &lt;FeatureLayer majorVersion="1" minorVersion="0"&gt;
            &lt;GlobalSettings enableTimeDependency="true"/&gt;
            &lt;Themes&gt;
              &lt;Theme id="PTV_PreferredRoutes" enabled="false" priorityLevel="0"&gt;
                &lt;PropertyValue id='preferredRouteType' value='9'/&gt;
                &lt;PropertyValue id='preferredRouteType' value='17'/&gt;
                &lt;PropertyValue id='preferredRouteType' value='23'/&gt;
            &lt;/Theme&gt;
              &lt;Theme id="PTV_TruckAttributes" enabled="false" priorityLevel="0"/&gt;
              &lt;Theme id="PTV_SpeedPatterns" enabled="false" priorityLevel="0"/&gt;
            &lt;/Themes&gt;
          &lt;/FeatureLayer&gt;
          &lt;Routing majorVersion="2" minorVersion="0"&gt;
            &lt;Course&gt;
              &lt;AdditionalDataRules enabled="true" /&gt;
              &lt;DynamicRouting limitDynamicSpeedToStaticSpeed="false" /&gt;
            &lt;/Course&gt;
            &lt;Vehicle&gt;
              &lt;Load hazardousGoodsType="HAZARDOUS" /&gt;
			  &lt;Physical&gt;
